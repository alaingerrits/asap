<div class="page-header">
  <%= link_to backlogitems_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_backlogitem_path(@backlogitem), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show backlogitem</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @backlogitem.name %></dd>

  <dt>Description:</dt>
  <dd><%= @backlogitem.description %></dd>

  <dt>Size:</dt>
  <dd><%= @backlogitem.size %></dd>

  <dt>Targetdate:</dt>
  <dd><%= @backlogitem.targetdate %></dd>

</dl>
